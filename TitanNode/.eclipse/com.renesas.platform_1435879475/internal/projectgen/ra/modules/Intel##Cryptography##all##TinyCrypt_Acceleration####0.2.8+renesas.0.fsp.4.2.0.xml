<?xml version="1.0" encoding="UTF-8"?>
<raModuleDescription>
    <config id="config.middleware.tinycrypt_accel" path="fsp_cfg/rm_tinycrypt_port_cfg.h" version="0">
        <property default="config.middleware.tinycrypt_accel.param_checking_enable.bsp" display="Parameter Checking" id="config.middleware.tinycrypt_accel.param_checking_enable" description="If selected code for parameter checking is included in the build.">
            <option display="Default (BSP)" id="config.middleware.tinycrypt_accel.param_checking_enable.bsp" value="(BSP_CFG_PARAM_CHECKING_ENABLE)"/>
            <option display="Enabled" id="config.middleware.tinycrypt_accel.param_checking_enable.enabled" value="(1)"/>
            <option display="Disabled" id="config.middleware.tinycrypt_accel.param_checking_enable.disabled" value="(0)"/>
        </property>
        <content>
#ifdef __cplusplus
extern "C" {
#endif

    #define RM_TINYCRYPT_PORT_CFG_PARAM_CHECKING_ENABLE ${config.middleware.tinycrypt_accel.param_checking_enable}

#ifdef __cplusplus
}
#endif
        </content>
    </config>

    <module config="config.middleware.tinycrypt_accel" display="Security|TinyCrypt (H/W Accelerated)" id="module.middleware.tinycrypt_accel" common="1" version="1" url="https://github.com/01org/tinycrypt/blob/master/documentation/tinycrypt.rst">
        <constraint display="Requires SCE Driver">
          "${interface.driver.sce}" === "1"
        </constraint>
        <!-- uECC documentation (http://kmackay.ca/micro-ecc/) requires only 672 bytes of stack but the mcuboot application requires about 1000 bytes of stack -->
        <constraint display="A minimum stack of 2K (0x800) is required. If used in an RTOS thread, the thread stack should instead be at least 0x800.">(("${config.bsp.common.main}" &gt;= 0x800)) || (testExists("${interface.rtos.rtos}") )
        </constraint>
        <requires id="module.middleware.tinycrypt_accel.requires.port" interface="interface.middleware.rm_tinycrypt_port" visible="false" find="true" display="HW Acceleration" />
        <requires id="module.middleware.tinycrypt_accel.requires.ra2" interface="interface.mcu.crypto_ra2" visible="false" find="true" display="RA2 AES Engine" />
        <provides interface="interface.driver.tinycrypt" />
        <provides interface="interface.driver.tinycrypt_acceleration" />
        <includes>
        #include "rm_tinycrypt_port.h"
        #include "tinycrypt/cbc_mode.h"
        #include "tinycrypt/ccm_mode.h"
        #include "tinycrypt/cmac_mode.h"
        #include "tinycrypt/constants.h"
        #include "tinycrypt/ctr_mode.h"
        #include "tinycrypt/ctr_prng.h"
        #include "tinycrypt/ecc_dh.h"
        #include "tinycrypt/ecc_dsa.h"
        #include "tinycrypt/ecc_platform_specific.h"
        #include "tinycrypt/ecc.h"
        #include "tinycrypt/hmac_prng.h"
        #include "tinycrypt/hmac.h"
        #include "tinycrypt/sha256.h"
        #include "tinycrypt/utils.h"
        </includes>
        <declarations>
        </declarations>
    </module>
</raModuleDescription>
